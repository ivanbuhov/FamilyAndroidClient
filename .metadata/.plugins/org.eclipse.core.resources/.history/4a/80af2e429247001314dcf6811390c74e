package com.buhov.FamilyHttpClient;

import com.buhov.FamilyHttpClient.Entities.FamilyServiceError;

public class FamilyHttpResponse<T> {
	
	private boolean successfull;
	private T result;
	private FamilyServiceError error;
	
	private FamilyHttpResponse(boolean successfull, T result, FamilyServiceError error) {
		this.result = result;
	}
	
	public T getResult() {
		return this.result;
	}
	
	public boolean isSuccessfull() {
		return this.successfull;
	}

}
